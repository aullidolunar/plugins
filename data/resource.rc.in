#include "resource.h"

LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
1 VERSIONINFO
    FILEVERSION     @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,@PROJECT_VERSION_TWEAK@
    PRODUCTVERSION  @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,@PROJECT_VERSION_TWEAK@
    FILEOS          VOS_UNKNOWN
    FILETYPE        VFT_UNKNOWN
    FILESUBTYPE     VFT2_UNKNOWN
    FILEFLAGSMASK   VS_FF_SPECIALBUILD
    FILEFLAGS       VS_FF_PRIVATEBUILD
{
    BLOCK "StringFileInfo"
    {
        BLOCK "080AFDE9"
        {
            VALUE "Comments", "Welcome to the crossplatform world"
            VALUE "CompanyName", "@AUTHOR@"
			VALUE "FileDescription", "@_PD@"
            VALUE "FileVersion", "@PROJECT_VERSION@"
            VALUE "InternalName", "@PROJECT_NAME@"
			VALUE "LegalCopyright", "@EMAIL@"
            VALUE "ProductVersion", "@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@"
        }
    }
    BLOCK "VarFileInfo"
    {
        VALUE "Translation", 0x080A, 0xFDE9
    }
}

LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
IDD_DIALOG1 DIALOG 0, 0, 264, 45
STYLE DS_3DLOOK | DS_CENTER | DS_MODALFRAME | DS_SHELLFONT | WS_CAPTION | WS_VISIBLE | WS_POPUP | WS_SYSMENU
CAPTION "Dialog"
FONT 8, "Ms Shell Dlg"
{
    EDITTEXT        IDC_ENTRY, 7, 17, 195, 14, ES_AUTOHSCROLL, WS_EX_LEFT
    LTEXT           "Static", IDC_LABEL, 7, 7, 195, 9, SS_LEFT, WS_EX_LEFT
    PUSHBUTTON      "Cancel", IDCANCEL, 208, 24, 50, 14, 0, WS_EX_LEFT
    DEFPUSHBUTTON   "OK", IDOK, 208, 7, 50, 14, 0, WS_EX_LEFT
}
